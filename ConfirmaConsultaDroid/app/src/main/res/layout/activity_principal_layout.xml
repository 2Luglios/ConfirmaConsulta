<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/botoes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:orientation="horizontal"
        android:padding="10dp">

        <ImageButton
            android:id="@+id/btnMeusDados"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.3"
            android:background="#00ffffff"
            android:src="@drawable/meus_dados"
            android:text="Principal"
            app:srcCompat="@drawable/meus_dados" />

        <ImageButton
            android:id="@+id/btnCalendario"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.4"
            android:background="#00ffffff"
            android:src="@drawable/agenda"
            android:text="UsuÃ¡rios"
            app:srcCompat="@drawable/agenda" />

        <ImageButton
            android:id="@+id/btnNotificacoes"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.3"
            android:background="#00ffffff"
            android:src="@drawable/com_not"
            android:text="Agenda"
            app:srcCompat="@drawable/com_not" />

    </LinearLayout>

    <FrameLayout
        android:id="@+id/fragment_place"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_above="@+id/botoes">

    </FrameLayout>

</RelativeLayout>